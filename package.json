{
	"name": "auth-service",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"update-packages": "yarn-check -u",
		"husky:prepare": "husky install && chmod ug+x .husky/*",
		"lint:check": "eslint src",
		"lint:fix": "yarn lint:check --fix",
		"lint:staged": "eslint --fix",
		"format:check": "prettier --check .",
		"format:write": "prettier --write .",
		"compodoc:build": "compodoc -p tsconfig.doc.json",
		"compodoc:serve": "compodoc -s",
		"prebuild": "rimraf dist",
		"build:dev": "NODE_ENV=dev nest build",
		"build:test": "NODE_ENV=dev nest build",
		"build:prod": "NODE_ENV=prod nest build",
		"format": "prettier --write \"src//*.ts\" \"test//*.ts\"",
		"start": "nest start",
		"start:dev-prod": "NODE_ENV=prod nest start --webpack --webpackPath webpack.config.js --watch",
		"start:dev": "nest start --webpack --webpackPath webpack.config.js --watch",
		"start:ngrok": "NODE_ENV=ngrok yarn start:dev",
		"start:debug": "yarn start:dev --debug",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}//*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@apollo/server": "4.11.3",
		"@nestjs/apollo": "12.2.2",
		"@nestjs/axios": "3.1.3",
		"@nestjs/common": "10.4.15",
		"@nestjs/core": "10.4.15",
		"@nestjs/event-emitter": "^2.1.1",
		"@nestjs/graphql": "12.2.2",
		"@nestjs/jwt": "^10.1.0",
		"@nestjs/microservices": "^10.4.15",
		"@nestjs/platform-express": "10.4.15",
		"@nestjs/platform-fastify": "10.4.15",
		"@nestjs/schedule": "^4.1.2",
		"@nestjs/swagger": "8.1.1",
		"@nestjs/typeorm": "^10.0.0",
		"@raydium-io/raydium-sdk": "^1.3.1-beta.58",
		"@raydium-io/raydium-sdk-v2": "^0.1.102-alpha",
		"@solana/web3.js": "^1.98.0",
		"add": "^2.0.6",
		"axios": "1.7.9",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"crypto-js": "^4.1.1",
		"dataloader": "^2.2.3",
		"graphql": "16.10.0",
		"graphql-scalars": "1.24.0",
		"graphql-type-json": "^0.3.2",
		"helius-sdk": "^1.4.1",
		"pg": "8.13.1",
		"reflect-metadata": "0.2.2",
		"rimraf": "6.0.1",
		"rxjs": "^7.8.1",
		"sharp": "0.33.5",
		"telegram": "2.26.16",
		"typeorm": "0.3.20",
		"yarn": "^1.22.21"
	},
	"devDependencies": {
		"@nestjs/cli": "10.4.9",
		"@nestjs/schematics": "10.2.3",
		"@nestjs/testing": "10.4.15",
		"@types/bcryptjs": "^2.4.6",
		"@types/express": "5.0.0",
		"@types/jest": "29.5.14",
		"@types/node": "22.10.5",
		"@types/sharp": "0.32.0",
		"@types/supertest": "6.0.2",
		"@types/uuid": "10.0.0",
		"@typescript-eslint/eslint-plugin": "7.1.1",
		"@typescript-eslint/parser": "7.1.1",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-simple-import-sort": "12.0.0",
		"eslint-plugin-sonarjs": "0.24.0",
		"eslint-plugin-unicorn": "51.0.1",
		"eslint-plugin-unused-imports": "3.1.0",
		"husky": "9.1.7",
		"jest": "^29.5.0",
		"lint-staged": "15.3.0",
		"prettier": "3.4.2",
		"pretty-quick": "4.0.0",
		"source-map-support": "^0.5.21",
		"supertest": "7.0.0",
		"ts-jest": "29.2.5",
		"ts-loader": "9.5.2",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typegram": "^5.2.0",
		"typescript": "5.7.3",
		"webpack": "^5.97.1",
		"yarn-check": "^0.0.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
